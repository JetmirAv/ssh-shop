<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fiek.controllers.profile.ListAddressController">
   <children>
      <JFXButton fx:id="createAddressId" onMouseClicked="#clickHandler" prefHeight="54.0" prefWidth="216.0" style="-fx-background-color: #129084;" text="Create address" textFill="WHITE">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" />
         </padding>
         <font>
            <Font name="Arial Black" size="13.0" />
         </font>
      </JFXButton>
      <ScrollPane fx:id="container" prefHeight="264.0" prefWidth="240.0" styleClass="address-scrollpane">
         <content>
            <VBox fx:id="addressList" prefHeight="300.0" prefWidth="225.0" spacing="2.0" stylesheets="@../../styles/style.css">
               <children>
         </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
