<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="600.0" prefWidth="500.0" styleClass="base-root" stylesheets="/org/fiek/styles/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fiek.controllers.auth.SignInController">
    <children>
        <JFXTextField fx:id="username" alignment="CENTER" focusColor="WHITE" labelFloat="true" layoutX="130.0" layoutY="200.0" promptText="Username" styleClass="log-in-information-field" unFocusColor="WHITE" />
        <JFXPasswordField fx:id="password" alignment="CENTER" focusColor="WHITE" labelFloat="true" layoutX="130.0" layoutY="291.0" promptText="Password" styleClass="log-in-information-field" unFocusColor="WHITE" />
        <JFXButton fx:id="logIn" layoutX="141.0" layoutY="369.0" onAction="#logInHandler" prefHeight="70.0" prefWidth="220.0" styleClass="info-getter-button" text="Log In" />
        <JFXButton layoutX="174.0" layoutY="461.0" onAction="#registerHandler" prefHeight="25.0" prefWidth="154.0" ripplerFill="WHITE" styleClass="login-register-switch-button" text="Create New Account" textFill="#4d4d4d" />
        <Label layoutX="150.0" layoutY="79.0" styleClass="pane-info" text="Log in" />

    </children>
</AnchorPane>
